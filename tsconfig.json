{
	"compileOnSave": true,
	"compilerOptions": {
		"alwaysStrict": true,
		"lib": ["esnext"],
		"strict": true,
		"module": "commonjs",
		"noUnusedParameters": true,
		"outDir": "./dist",
		"sourceMap": true,
		"types": ["node", "discord.js", "klasa"],
		"declaration": true,
		"noUnusedLocals": true,
		"removeComments": false,
		"target": "esnext"
	},
	"exclude": [
		"node_modules",
		"bin",
		"examples",
		"scripts"
	],
	"include": [
		"./src/**/*"
	]
}
